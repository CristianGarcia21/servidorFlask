// Diagrama NFA
strict digraph {
	rankdir=LR
	node [shape=circle]
	0 -> 1 [label=e]
	1 -> 2 [label=a]
	2 -> 1 [label=e]
	2 -> 3 [label=e]
	0 -> 3 [label=e]
	0 -> 1
	3 -> 4 [label=e]
	4 -> 5 [label=a]
	5 -> 4 [label=e]
	5 -> 6 [label=e]
	3 -> 6 [label=e]
	6 -> 7 [label=a]
	3 -> 4
	7 -> 8 [label=b]
	3 -> 4
	0 -> 1
	8 -> 9 [label=e]
	9 -> 10 [label=b]
	10 -> 9 [label=e]
	10 -> 11 [label=e]
	8 -> 11 [label=e]
	8 -> 9
	11 [shape=doublecircle]
}
